public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
   List<sObject> sobList =  new List<sObject>();

   if(!accounts.isEmpty() || accounts != null)
   {
       for(Account ac : accounts)
       sobList.add(ac);
   }
   else
   {
        sobList.add(new List<Account>());
   }
   if(!contacts.isEmpty() || contacts == null)
   {
       for(Contact con : contacts)
       sobList.add(con);
   }
   
     else
   {
   sobList.add(new List<Contact>());
   }

   return sobList;
}