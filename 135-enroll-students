public void enrollStudents(List<String> emails, String className) {
    
   apxio__Class__c studentClass = [select Id from apxio__Class__c where Name = :className LIMIT 1];
 
   List<apxio__Student__c> studentList = [Select Id,apxio__Email__c form apxio__Student__c where apxio__Email__c IN emails ];
   
   List<apxio__Class_Enrollment__c> enrollmentList = new List<apxio__Class_Enrollment__c>();

   for( apxio__Student__c student : studentList)
   {
       apxio__Class_Enrollment__c enrollment = new apxio__Class_Enrollment__c();
       apxio__Class_Enrollment__c.apxio__Student__c = student.Id;
       apxio__Class_Enrollment__c.apxio__Offered_Class__c = studentClass.Id;
       enrollmentList.add(enrollment);
   }

   insert enrollmentList;
}