public List<Integer> twoSum(List<Integer> nums, Integer target){

   if (nums == null  || target == null)|| nums.size() == 0) {
        return null; 
    }
    List<Integer> twoNum = new List<Integer>();
   
  for(Integer i = 0 ; i < nums.size()-1 ; i++)
  {
      if(nums[i]+nums[i+1] == target)
      {
          twoNum.add(i);
          twoNum.add(i+1);
          return twoNum;
      }
  }

  return null;
}