public static List<Integer> squareList(List<Integer> nums){
   Integer temp;
   
   
   for(Integer i = 0 ; i< nums.size();  i++)
   {
       nums.set(i,nums[i]*nums[i]);
   }
   
   nums.sort();
    for(Integer i = 0 ; i < nums.size() ; i++)
   {
       for(Integer j = i ; j < nums.size() ; j++ )
       {
           if(nums[i] >= nums[j])
           {
                temp = nums[i];
                nums[i] = nums[j];
                nums[j] = temp;
           }

       }
   }
}