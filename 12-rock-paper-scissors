public Integer rockPaperScissors(String player1, String player2) {
  
  if(player1 == player2)
  return 0;

  if(player1=='rock')
  {
      if(player2=='scissors')
      return 1;
      else 
      return 2;  
  }
  else if(player1=='paper')
  {
      if(player2=='scissors')
      return 2;
      else 
      return 1;
     
  }

  else
  {
      if(player2=='paper')
      return 1;
      else 
      return 2;
  }

}