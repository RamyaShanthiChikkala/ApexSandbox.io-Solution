public Integer secondLargest(List<Integer> numbers) {
  
  if(numbers.size() < 2) return 0;
  Integer max = numbers[0]? -2,147,483,648 :  -2147483647 - 1 : numbers[0]; 
  Integer secondLargestNum = numbers[1];
  if(secondLargestNum > max)
  secondLargestNum = max;
 for(Integer i = 2 ; i < numbers.size(); i++)
 {
     if(numbers[i] > max)
     {
         secondLargestNum = max;
         max = numbers[i];
     }
    else if(numbers[i] >= secondLargestNum && (numbers[i] != max)) 
    {
        secondLargestNum = numbers[i];
    }
    else if(max == secondLargestNum)
    secondLargestNum = numbers[i];

 }
 return secondLargestNum;
}