public Id classFromCourse(String courseName) {
  
  apxio__Course__c course = [SELECT Id, Name from apxio__Course__c WHERE Name =: courseName LIMIT 1];
  apxio__Class__c classNew = new apxio__Class__c();
      
  classNew.apxio__Course__c = course.Id;
  classNew.Name = course.Name;
  insert classNew;

  return classNew.Id;

}