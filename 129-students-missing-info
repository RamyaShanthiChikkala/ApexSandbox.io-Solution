public List<apxio__Student__c> selectStudentsWithoutContactInfo() {
  
  List<apxio__Student__c> studentInfoMiss = List<apxio__Student__c>();

  studentInfoMiss = [SELECT Id , Name from apxio__Student__c where apxio__Active__c=true AND (apxio__Email__c = null OR apxio__Phone__c = null) ORDER by Name ASC ];

  return studentInfoMiss;
  
}