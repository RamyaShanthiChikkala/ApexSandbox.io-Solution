public Map<String, Decimal> industrySummary(List<Account> accounts) {

   Map<String,Decimal> indSum = new Map<String,Decimal>(); 
    Decimal sum = 0;
    for(Account ac : accounts)
    {
        if(indSum.containsKey(ac.Industry))
     

             sum = indSum.get(ac.Industry) + ac.AnnualRevenue;
        else
        
             sum = indSum.get(ac.Industry);
            indSum.put(ac.Industry,ac.AnnualRevenue);
       
        indSum.put(ac.Industry,sum);
    }

    return indSum;

}