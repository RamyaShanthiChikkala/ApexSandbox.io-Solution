public Boolean duplicateContacts(Contact c1, Contact c2) {
    if(c1 == null || c2 == null)
    return null;

    if(c1.Email == null || c2.Email == null || c1.Phone==null || c2.Phone == null)
    return false;
    else if( (c1.Email != c2.Email && c1.Phone == c2.Phone)||(c1.Phone != c2.Phone && c1.Email == c2.Email) )
    return true;
    else
    return false;
    
}