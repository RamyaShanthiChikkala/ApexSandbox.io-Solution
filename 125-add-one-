public void plusOne(List<Integer> numbers) {
    
Integer carry = 1;

for(Integer i = numbers.size()- 1 ; i >= 0 ; i++)
{
    Integer sum = numbers.get(i) + carry;
    if(sum > 9)
    {
        carry = 1;
        numbers.set(i,0);
    }
    else
    {
        carry = 0;
        numbers.set(i, sum);
        break;
    }

}
 if (carry == 1) {
            numbers.add(0, 1);
        }

}