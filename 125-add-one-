public void plusOne(List<Integer> numbers) {
    
for(Integer i = numbers.size() -1 ; i > 1; i--)
{
    Integer digit = numbers[i];
    if(digit >= 9)
    {
        numbers.set(i,digit + 1);
        digit = 1 + numbers.get(i-1);
    }
    else
    {
        numbers.set(i,digit+1);
        return;
    }

}


}